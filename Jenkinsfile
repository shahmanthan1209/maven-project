pipeline {
    agent any 
        stages {
            stage ('init') {
                steps {
                    echo "Initialize pipeline"
                    
                }
            }
            
            stage ('build') {
                steps {
                    echo "Build started"
                    
                }
            }
            
            stage ('deploy') {
                steps {
                    echo "Deployment Started"
                    
                }
            }
        }
    
}